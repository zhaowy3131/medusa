{
  "name": "@medusajs/framework",
  "version": "0.0.2",
  "description": "Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config/index.js",
    "./logger": "./dist/logger/index.js",
    "./database": "./dist/database/index.js",
    "./subscribers": "./dist/subscribers/index.js",

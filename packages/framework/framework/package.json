    "directory": "packages/framework/framework"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Medusa",
  "license": "MIT",
  "scripts": {
    "watch": "tsc --watch -p ./tsconfig.build.json",
    "watch:test": "tsc --build tsconfig.spec.json --watch",
